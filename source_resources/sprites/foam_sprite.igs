	<medium>
		<uid>7</uid>
		<name>medium1</name>
			<precedence>2</precedence>
			<basic>
				<ior>1</ior>
				<cauchy_b_coeff>0</cauchy_b_coeff>
				<max_extinction_coeff>200</max_extinction_coeff>
				<fast_sss>false</fast_sss>
				<absorption_coefficient>
					<constant>
						<uniform>
							<value>0</value>
						</uniform>
					</constant>
				</absorption_coefficient>
				<subsurface_scattering>
						<scattering_coefficient>
							<shader>
									<shader><![CDATA[	
          
				def eval(vec3 pos) vec3 :
					let
						rad = dist(pos, vec3(0, 0, 0))
						env = 1.0 - smoothstep(0.2, 1.2, rad)
						#env = exp(-rad*rad * 25.0)


						#v = max(0.0, pos.x)
						#v = min(1.0, (fbm(pos * 30.0, 7)*env - 0.4)*3.0) * 200
						v = max(0.0, min(1.0, (fbm(pos * 20.0, 4) + 0.7*env - 1.6)*10.0)) * 200

						
					in
						vec3(v)
					
					]]></shader>
								<wavelength_dependent>false</wavelength_dependent>
							</shader>
						</scattering_coefficient>
						<phase_function>
							<uniform>
							</uniform>
						</phase_function>
				</subsurface_scattering>
			</basic>
	</medium>

